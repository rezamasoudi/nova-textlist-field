{
    "name": "masoudi/nova-textlist-field",
    "description": "text list field for the Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "tags",
        "input"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Masoudi\\Nova\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masoudi\\Nova\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
